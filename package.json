{
    "name": "ts-draft",
    "version": "1.0.0",
    "description": "Draft of TypeScript project",
    "main": "src/index.ts",
    "scripts": {
        "db": "prisma/db",
        "db:reuse": "docker-compose up -d",
        "build:resolvers": "prisma generate",
        "postinstall": "npm run build:resolvers",
        "mig:new": ". prisma/url start && prisma migrate dev --create-only --name",
        "mig:start": ". prisma/url start && prisma migrate deploy",
        "mig:test": ". prisma/url test && prisma migrate deploy",
        "psql": "docker-compose exec db psql -U postgres -d start",
        "psql:data": "docker-compose exec -T db psql -U postgres -d start",
        "lint": "eslint --max-warnings 0 --fix \"**/*.{js,json,ts}\" && tsc --noEmit",
        "test": ". prisma/url test && jest --runInBand",
        "start": ". prisma/url start && LOG_LEVEL=debug nodemon -w src --ext ts --exec ts-node -r tsconfig-paths/register src/index.ts",
        "build:schema": "ts-node -r tsconfig-paths/register scripts/buildSchema.ts",
        "build": "npm run build:resolvers && npm run build:schema",
        "version": "scripts/npm_version.sh"
    },
    "author": "Denis Ryzhkov",
    "license": "ISC",
    "dependencies": {
        "@isaacs/ttlcache": "^1.2.1",
        "@prisma/client": "^4.7.1",
        "apollo-server": "^3.10.3",
        "apollo-server-lambda": "^3.10.3",
        "class-validator": "^0.13.2",
        "graphql": "^15.3.0",
        "graphql-fields": "^2.0.3",
        "pg-format": "^1.0.4",
        "reflect-metadata": "^0.1.13",
        "tslib": "^2.4.0",
        "type-graphql": "^1.1.1",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/graphql-fields": "^1.3.4",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.11.4",
        "@types/pg-format": "^1.0.2",
        "@types/prettier": "^2.7.1",
        "@types/validator": "^13.7.7",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "graphql-import-node": "^0.0.5",
        "jest": "^29.1.2",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "prisma": "^4.7.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typegraphql-prisma": "^0.23.1",
        "typescript": "^4.8.4"
    }
}
